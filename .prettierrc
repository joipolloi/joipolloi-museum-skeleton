{
    "trailingComma": "es5",
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 4,
    "plugins": [
        "./node_modules/prettier-plugin-twig-melody"
    ],
    "twigPrintWidth": 120,
    "twigMultiTags": [
        "nav,endnav",
        "switch,case,default,endswitch",
        "ifchildren,endifchildren",
        "cache,endcache",
        "js,endjs"
    ]
}
